{
  "name": "ytshuffle",
  "type": "module",
  "private": true,
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "format": "prettier --write .",
    "build": "tsc && vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "postdeploy": "git push",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0 src",
    "test": "vitest",
    "lambda1": "yarn tsc --target esnext --moduleResolution nodenext lambda/getPlaylist.ts",
    "lambda2": "yarn tsc --target esnext --moduleResolution nodenext lambda/getPlaylistContents.ts",
    "lambda": "yarn lambda1 && yarn lambda2"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "eslint": "^9.5.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "localforage": "^1.10.0",
    "mobx": "^6.12.4",
    "mobx-react": "^9.1.1",
    "mobx-state-tree": "^5.1.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-youtube": "^10.1.0",
    "timeago.js": "^4.0.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/youtube-player": "^5.5.7",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.20",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "next",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gh-pages": "^6.0.0",
    "postcss": "^8.4.47",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.0.0",
    "vitest": "^2.0.5"
  }
}
